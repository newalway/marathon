{% extends 'ProjectBundle::admin.html.twig'%}

{% block javascripts %}
  {{ parent() }}
{% endblock %}

{% use "ProjectBundle:AdminAboutUs:blocks.html.twig" %}
{% use "ProjectBundle::flash_message.html.twig" %}
{% block content %}
<div class="row">
	<div class="col-md-12">
	  <div class="box box-primary">
	    <div class="box-header">
	      <h3 class="box-title">Update</h3>
	    </div><!-- /.box-header -->
	    <!-- form start -->
      <form action="{{path('admin_about_us_update', {'id': form.vars.value.id})}}" method="post" role="form" enctype="multipart/form-data" class="form-horizontal">
        {{form_widget(form._token)}}
        {% include 'ProjectBundle:AdminAboutUs:_form.html.twig' %}
      </form>
    </div><!-- /.box -->
  </div>
</div>
{% endblock %}
